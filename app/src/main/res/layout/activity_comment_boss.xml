<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="ucsd.cse110fa16.group14.ipark.CommentBoss">


    <RadioGroup
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:id="@+id/radioGroup"
        >

    </RadioGroup>

    <ListView
        android:id="@+id/list_view2"
        android:layout_width="350dp"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:layout_height="380dp" />

    <TextView
        android:text="USER COMMENTS"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="26dp"
        android:textStyle="bold"
        android:id="@+id/textView30"
        android:layout_marginBottom="16dp"
        android:layout_above="@+id/list_view2"
        android:layout_alignStart="@+id/list_view2"
        android:layout_marginStart="11dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/new_home"
        android:background="#FFFFFF"
        android:id="@+id/homeBB"
        android:layout_below="@+id/list_view2"
        android:layout_alignEnd="@+id/list_view2" />


</RelativeLayout>
